.index-sns
  padding: 10px 0
  margin-bottom: 30px
  min-height: 200px
  h2
    font: 15px Arial, Helvetica, sans-serif
    color: #072
    margin: 0 0 12px 0
    line-height: 150%
    .p1
      font-size: 12px
      color: #666666
  .index-sns-advertising
    width: 950px
    height: 90px
  .index-sns-side
    float: right
    width: 265px
    margin-left: 30px
    .app-moment
      margin: 10px 0
      a,img
        vertical-align: middle
    .online
      ul
        margin-top: -4px
      li
        margin-top: 10px
        color: #999999
      li:first-child
        margin-top: 0
      .title
        margin-bottom: 4px
        line-height: 1.45
  .index-sns-main
    overflow: hidden
    .mod
      margin-bottom: 30px
      .album
        float: left
        width: 350px
        margin-right: 30px
        color: #999
        font-size: 0
        letter-spacing: -0.31em
        ul
          margin: -10px 0 0 -10px
        li
          letter-spacing: normal
          word-spacing: normal
          width: 170px
          vertical-align: top
          font-size: 12px
          margin: 10px 0 0 10px
        .pic
          display: inline-block
          width: 170px
          max-height: 170px
          vertical-align: middle
          white-space: nowrap
          overflow: hidden
      .notes
        width: 275px
        float: left
        color: #666
        .author
          color: #999
        .desc
          margin: 0
        li
          margin-top: 12px
        li:first-child
          margin-top: 0
  a:hover
    background-color: #37a
